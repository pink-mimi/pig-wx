.container{
  width: 100%;
  padding: 0;
  margin: 0;
  position: relative;
}
.index_swiper{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
}
.swiper_img{
  width: 100%;
}
.index_content{
  width: 100%;
  margin-top: 480rpx;
}

/* 修改swiper指示点位置 */
.wx-swiper-dots {
  position: relative;
  left: unset !important;
  right: 0;

}
.wx-swiper-dots.wx-swiper-dots-horizontal {
  margin-bottom: 30rpx;
}



/* content内容 */
.index_content{
  width: 100%;
  background: white;
  position: relative;
}
.index_search{
  width: 90%;
  margin-left: 5%;
  box-sizing: border-box;
  position: absolute;
  top: 0;
  left: 0;
  transform: translateY(-50%);
  position: relative;
}
.search{
  width: 100%;
  height: 80rpx;
  border: 1px solid #cccccc;
  border-radius: 10rpx;
  background: white;
  box-sizing: border-box;
  padding: 10rpx  130rpx ;
}
.index_search_p1{
  position: absolute;
  box-sizing: border-box;
  top: 0;
  left:0;
  line-height: 70rpx;
}
.icon_seach1{
  vertical-align: middle;
  font-size: 32rpx;
  padding: 0 10rpx 0 20rpx;
}
.index_search_text1{
  vertical-align: middle;
  font-size: 28rpx;
  font-weight: 700;
}
.index_search_p2{
  position: absolute;
  box-sizing: border-box;
  top: 0;
  right:0;
  margin: 10rpx 0;
  line-height: 50rpx;
  border-left: 1rpx solid rgba(0, 0, 0, 0.3);
}
.icon_seach2{
  vertical-align: middle;
  font-size: 32rpx;
  padding-left: 20rpx;
}
.index_search_text2{
  font-size: 28rpx;
  vertical-align: middle;
  color: #757575;
  font-weight: 700;
  padding: 0 20rpx;
}


/* input下面的宫格 */

.index_gongge{
  padding: 0 40rpx ;
  display: flex;
  flex-wrap: wrap;
  box-sizing: border-box;
}
.index_gongge_item{
  box-sizing: border-box;
  width: 20%;
  text-align: center;
  font-size: 24rpx;
  margin-bottom: 10rpx;
}
.b{
  font-weight: 700;
}
.gongge_item_img{
  width: 80rpx;
  height: 80rpx;
}


/* city */
.index_city{
  box-sizing: border-box;
  display: flex;
  margin: 0 40rpx;
  flex-wrap: nowrap;
  overflow-x: scroll;
  overflow-y: hidden;
  white-space: nowrap;
  padding: 20rpx 0;
}
.index_city::-webkit-scrollbar {
  display: none;
  background-color: transparent;
}
.city{
  width: 20%;
  border: 1rpx solid #e0e0e0;
  flex-shrink:0;
  margin-right: 15rpx;
  text-align: center;
  font-size: 28rpx;
  font-weight: 700;
  line-height: 60rpx;
  border-radius: 10rpx;
  background-color: white;
}
.city_active{
  color: #ff4081;
  border-color: #ff4081;
}

/* goods房型 */
.index_house{
  margin: 0 40rpx;
}
.house_list{
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}
.house_item{
  width: 49%;
  padding-bottom: 30rpx;
}
.house_item_img{
  width: 100%;
  border-radius: 10rpx;
  height: 200rpx;
  display: block;
}
.house_item_content1{
  display: flex;
  justify-content: space-between;
  margin: 10rpx 0;
}
.house_price{
  font-size: 32rpx;
  font-weight: 700;
}
.dian{
  font-size: 24rpx;
  color: #dbdbdb;
  padding: 0 10rpx;
}
.house_dwell{
  font-size: 24rpx;
  color: #dbdbdb;
}
.star{
  font-size: 24rpx;
  font-weight: 700;
  color: #00ffff;
}
.house_grade{
  font-size: 24rpx;
}
.house_item_content2{
  font-size: 28rpx;
  color: #757575;
}

.house_more{
  width: 100%;
  line-height: 70rpx;
  font-size: 30rpx;
  font-weight: 700;
  text-align: center;
  background-color: white;
  border: 1rpx solid #ff4081;
  border-radius: 10rpx;
}

/* swiper轮播 */
.index_content_swiper{
  margin: 30rpx 40rpx 0;
}

.swiper_item{
  width: 100%;
  height: 100%;
}
.swiper-item{
  width: 100%;
  height: 100%;
}
.index_content_swiper_img{
  border-radius: 10rpx;
  width:100%;
  height:100%;
}

/* 超值特惠 */

.index_discounts{
  margin: 40rpx 40rpx 0;
}
.discounts_title{
  font-size: 40rpx;
  font-weight: 700;
  margin-bottom: 20rpx;
}
.discounts_img{
  display: flex;
  justify-content: space-between;
}
.img_left,.img_right{
  width: 50%;
}
.img_left{
  padding-right: 7.5rpx;
  position: relative;
}
.img_right{
  padding-left: 7.5rpx;
}

.img_left1{
  display: block;
  width: 100%;
  height:320rpx;
}

.img_right1{
  display: block;
  width: 100%;
  height: 152.5rpx;
  padding-bottom: 15rpx;
  border-top-right-radius: 20rpx;
  position: relative;
}
.img_right2{
  display: block;
  width: 100%;
  height: 152.5rpx;
  border-bottom-right-radius: 20rpx;
  position: relative;
}
/* 定位 */
.img_left_p{
  position: absolute;
  top: 20rpx;
  left: 20rpx;
}
.img_left_p1{
  font-size: 32rpx;
  color: white;
}
.img_left_p2{
  font-size: 24rpx;
  color: white;
  background-color: blue;
}
.img_right1_p,.img_right2_p{
  position: absolute;
  top: 20rpx;
  left: 20rpx;
}
.img_right1_p1{
  font-size: 32rpx;
  color:white;
}
.img_right1_p2{
  font-size: 24rpx;
  color: white;
}

/* 小猪保障 */
.index_safeguard{
  margin: 100rpx 40rpx 0;
}
.text1{
  font-size: 32rpx;
  font-weight: 700;
  text-align: center;
  margin-bottom: 20rpx;
}
.text2{
  font-size: 28rpx;
  font-weight: 700;
  text-align: center;
}
.safeguard_logo{
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  box-sizing: border-box;
}
.safeguard_logo_item{
  width: 50%;
  box-sizing: border-box;
  text-align: center;
  margin-top: 100rpx;
}
.logo_item{
  text-align: center;
}
.logo_img{
  width: 100rpx;
  height: 100rpx;
}
.logo_text{
  text-align: center;
  font-size: 32rpx;
  margin-top: 10rpx;
  font-size: 28rpx;
}