.container{
  width: 100%;
  padding: 0;
  margin: 0;
  position: relative;
}
.index_swiper{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
}
.swiper_img{
  width: 100%;
}
.index_content{
  width: 100%;
  margin-top: 480rpx;
}

/* content内容 */
.index_content{
  width: 100%;
  background: pink;
  position: relative;
}
.index_search{
  width: 80%;
  margin-left: 10%;
  box-sizing: border-box;
  position: absolute;
  top: 0;
  left: 0;
  transform: translateY(-50%);
}
.search{
  width: 100%;
  height: 70rpx;
  border: 1px solid #cccccc;
  border-radius: 10rpx;
  background: white;
}
