/* pages/detail/detail.wxss */

.container{
  padding: 0;
  margin: 0;
  width: 100%;
  position: relative;
}
.detail_swiper{
  width: 100%;
}
.swiper_img{
  width: 100%;
  height: 450rpx;
}


/* 修改swiper指示点位置 */
.wx-swiper-dots {
  position: relative;
  left: unset !important;
  right: 50%;
  transform: translateX(50%) !important;
}
.wx-swiper-dots.wx-swiper-dots-horizontal {
  margin-bottom:60rpx;
}


/* content */
.detail_content{
  box-sizing: border-box;
  padding: 30rpx;
  background-color: white;
  position: absolute;
  top:400rpx;
  left: 0;
  width: 100%;
  z-index:9;
  border-radius: 50rpx;
  margin-bottom: 150rpx;
}
/* 房源名字 */
.house_name{
  font-size: 40rpx;
  font-weight:700;
}
/* 房源描述 */
.house_desc{
  display: flex;
  justify-content: space-between;
  margin: 50rpx 0;
}
.house_desc1{
  width: 50%;
  text-align: center;
  display: flex;
  justify-content: center;
}
.house_desc1_img{
  border: 1rpx solid #757575;
  border-radius: 50%;
  padding: 10rpx;
}
.house_desc1_img image{
  width: 60rpx;
  height: 60rpx;
  display: block;
}
.house_desc1_text{
  padding-left: 10rpx;
}
.house_desc1_text view{
  font-size: 24rpx;
  text-align: left;
  padding-top: 3rpx;
}

/* 房源地图 */
.house_map{
  height: 50rpx;
  background-color: skyblue;
}


/* 房源日历 */
.house_date{
  display: flex;
  justify-content: space-between;
  padding: 20rpx;
  background-color: #ffd2f3;
  border-radius: 10rpx;
  margin: 10rpx 0 50rpx 0;
}
.date_ru{
  width: 40%;
}
.date_li{
  width: 40%;
  text-align: center;
}

.yue,.ri{
  font-size: 40rpx;
}
.yue_text,.ri_text{
  font-size: 28rpx;
}
.text{
  color: #757575;
  font-size: 28rpx;
}
.arrow{
  font-size: 32rpx;
  text-align: right;
}

/* 设施 */
.facility_title{
  display: flex;
  justify-content: space-between;
  margin-bottom: 30rpx;
}
.facility_name{
  font-size: 40rpx;
  font-weight: 700;
}
.facility_all{
  font-size: 28rpx;
}
.facility_all text{
  vertical-align: middle;
}
.more{
  margin-left: 20rpx;
  vertical-align: middle;
  font-size: 32rpx;
  color: #cccccc;
}


.facility_content{
  padding: 20rpx;
  border: 1rpx solid rgba(100, 100, 100, 0.2);
  border-radius: 10rpx;
}
.basics,.bath,.electric{
  display: flex;
  justify-content: space-between;
}
.basics .title_name,.bath .title_name,.electric .title_name{
  width: 10%;
  text-align: center;
}
.bath{
  margin: 15rpx 0;
}
.basics view,.bath view,.electric view{
  width: 30%;
  text-align: left;
}
.title_name{
  font-size: 28rpx;
  font-weight: 700;
}
.basics view,.bath view,.electric view{
  font-size: 28rpx;
}
.ico{
  vertical-align: middle;
  font-size: 28rpx;
  color: #00ffff;
}
.sheshi_name{
  padding-left: 20rpx;
  vertical-align: middle;
}

/* 房东 */

.landlord_title{
  width: 100%;
  display: flex;
  justify-content: space-between;
}
.landlord_title_name{
  font-size: 40rpx;
  font-weight: 700;
}
.landlord_more text{
  font-size: 28rpx;
  vertical-align: middle;
}
.zhuye{
  font-size: 32rpx;
  vertical-align: middle;
  margin-left: 15rpx;
  color: #cccccc;
}

.landlord_jieshao{
  display: flex;
  padding: 50rpx 30rpx 10rpx 30rpx;
  border-bottom: 1rpx dashed #cccccc;
  margin-bottom: 10rpx;
}
.landlord_img image{
  width: 120rpx;
  height: 120rpx;
}
.landlord_info{
  margin-left: 20rpx;
}
.landlord_name{
  font-size: 32rpx;
  margin: 15rpx 0 5rpx 0;
}
.landlord_renzheng{
  font-size: 24rpx;
  padding: 5rpx 10rpx;
  border: 1rpx solid #00FFFF;
  border-radius: 5rpx;
  color: #00FFFF;
}
.landlord_desc{
  width: 100%;
}
.landlord_desc_text{
  width: 100%;
  font-size: 28rpx;
  color: #757575;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  line-height: 50rpx;
}
.landlord_desc_more{
  text-align: right;
  font-size: 28rpx;
  color: #964B00;
}

/* 预定须知 */
.notice{
  width: 100%;
  margin: 50rpx 0 0;
}
.notice_title{
  display: flex;
  justify-content: space-between;
}
.notice_title_name1{
  font-size: 40rpx;
  font-weight: 700;
}
.notice_title_name2{
  font-size: 28rpx;
}

.rule_all{
  font-size: 28rpx;
  vertical-align: middle;
  color: #757575;
}
.guize{
  font-size: 32rpx;
  color: #cccccc;
  vertical-align: middle;
}
.notice_list{
  padding: 30rpx;
}
.notice_item{
  display: flex;
  margin-bottom: 30rpx;
}
.notice_item_title{
  font-size: 28rpx;
  font-weight: 700;
  width: 30%;
}
.notice_item_desc{
  font-size: 28rpx;
  width: 70%;
  text-align: left;
}

/* 同类好房 */

.type_house{
  width: 100%;
  margin-top: 20rpx;
  /* background-color: pink; */
}
.type_house_title{
  font-size: 40rpx;
  font-weight: 700;
  margin-bottom: 30rpx;
}
.type_house_content{
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}

.type_house_item{
  padding: 5rpx;
  border-radius: 10rpx;
  width: 49%;
  box-sizing: border-box;
  background-color: white;
  margin-bottom: 10rpx;
  box-shadow: 0rpx 0rpx 10rpx rgba(0, 0, 0, 0.2);

}
.type_house_img image{
  width: 100%;
  border-radius: 10rpx;
  height: 300rpx;
  display: block;
}

.type_house_name{
  font-size: 28rpx;
  font-weight: 700;
  margin: 20rpx 0 10rpx;
  min-height: 76rpx;
}
.type_house_desc{
  font-size: 24rpx;
  color: #757575;
  margin-bottom: 10rpx;
}
.type_house_price{
  padding-bottom: 20rpx;
}
.type_house_price_new{
  font-size: 36rpx;
  font-weight: 700;
  color: #ff4081;
  margin-right: 10rpx;
}
.type_house_price_old{
  font-size: 24rpx;
  color: #757575;
}

/* .底部定位 */
.reserve{
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  background-color: white;
  display: flex;
  justify-content: space-between;
  z-index: 99;
  padding: 20rpx;
  box-sizing: border-box;
  border-top: 1rpx solid #cccccc;
}

.reserve_lj{
  padding: 10rpx 30rpx;
  line-height:60rpx;
  font-size: 36rpx;
  font-weight: 700;
  color: white;
  background-color: #ff4081;
  border-radius: 40rpx;
}

.function{
  display: flex;
  margin-left: 30rpx;

}
.chat,.collect{
  font-size: 24rpx;
}
.collect{
  margin-left: 20rpx;
}
.liaotian,.shoucang{
  font-size: 40rpx;
}

.reserve_price{
  font-size: 28rpx;
  line-height: 80rpx;
}
.reserve_price_ico{
  font-size: 24rpx;
  font-weight: 700;
  color: #ff4081;
}
.reserve_price_num{
  font-size: 40rpx;
  color: #ff4081;
  font-weight: 700;
}
